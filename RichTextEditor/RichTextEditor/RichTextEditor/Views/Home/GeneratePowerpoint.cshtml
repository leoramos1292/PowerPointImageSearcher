@using RichTextEditor.Models
@model RichTextEditorViewModel

@{
    ViewBag.Title = "GeneratePowerpoint";
}

<h2>PowerPoint Slide</h2>

<div>
    <div>
        @for (int i = 0; i < Model.ImagePaths.Count; i++)
        {
            <div class="col-sm cardStyle">
                <img class="card-img-top" style="width:200px; height:200px;" src="@Url.Content(Model.ImagePaths[i])" alt="Card image cap"><br />
            </div>
        }
    </div>
</div>

